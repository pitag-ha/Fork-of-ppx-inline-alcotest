(lang dune 2.0)
(package
 (name ppx_inline_alcotest)
 (synopsis "Inline tests backend for alcotest.")
 (description "Run inline tests in the style of Alcotest.")
 (depends
  (ppxlib (and (>= 0.22.0) :with-test))
  (metaquot (>= ~))
  (ocaml (>= 4.05.0))
  (alcotest (>= 1.0.0))))
(version 1.0.0)
(name ppx_inline_alcotest)
(generate_opam_files true)
